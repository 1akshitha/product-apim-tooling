            <stringProp name="script">import java.io.FileReader;
import net.minidev.json.JSONObject;
import net.minidev.json.parser.JSONParser;
import org.apache.commons.lang.StringUtils;
import org.apache.commons.codec.binary.Base64;

FileReader fileReader=new FileReader(&quot;conf/config.json&quot;);
BufferedReader bufferReader= new BufferedReader(fileReader);
String line=bufferReader.readLine();
String data = &quot;&quot;;
while (line != null) {
	data = data + line;
	line=bufferReader.readLine();
}
fileReader.close(); bufferReader.close(); //print(data);   
JSONParser parser = new JSONParser(JSONParser.MODE_JSON_SIMPLE);
JSONObject root = (JSONObject) parser.parse(data);
JSONObject[] intances = new JSONObject[2];
intances[0] = (JSONObject) root.get(&quot;currentInstance&quot;);
intances[1] = (JSONObject) root.get(&quot;newInstance&quot;);
for (int i = 1; i &lt; 3; i++) {
	vars.put(&quot;username&quot;+i,intances[i-1].getAsString(&quot;username&quot;)); 
	vars.put(&quot;password&quot;+i, intances[i-1].getAsString(&quot;password&quot;)); 
	vars.put(&quot;clientRegisterUrl&quot;+i, intances[i-1].getAsString(&quot;clientRegisterUrl&quot;)); 
	vars.put(&quot;accessTokenUrl&quot;+i, intances[i-1].getAsString(&quot;accessTokenUrl&quot;)); 
	vars.put(&quot;APIsPublisherUrl&quot;+i, intances[i-1].getAsString(&quot;APIsPublisherUrl&quot;)); 
	vars.put(&quot;APIsStoreUrl&quot;+i, intances[i-1].getAsString(&quot;APIsStoreUrl&quot;)); 
	vars.put(&quot;applicationsStoreUrl&quot;+i, intances[i-1].getAsString(&quot;applicationsStoreUrl&quot;)); 
	vars.put(&quot;subscriptionsStoreUrl&quot;+i, intances[i-1].getAsString(&quot;subscriptionsStoreUrl&quot;)); 
}
vars.put(&quot;numberOfAPIsToConsider&quot;,intances[0].getAsString(&quot;numberOfAPIsToConsider&quot;)); 
vars.put(&quot;numberOfApplicationsToConsider&quot;, intances[0].getAsString(&quot;numberOfApplicationsToConsider&quot;));

String name= vars.get(&quot;username1&quot;);
String pwd= vars.get(&quot;password1&quot;);
String input = name+&quot;:&quot;+pwd;
byte[] encryptedUid = Base64.encodeBase64(input.getBytes());
vars.put(&quot;base64Login1&quot;,new String(encryptedUid));

name= vars.get(&quot;username2&quot;);
pwd= vars.get(&quot;password2&quot;);
input = name+&quot;:&quot;+pwd;
encryptedUid = Base64.encodeBase64(input.getBytes());
vars.put(&quot;base64Login2&quot;,new String(encryptedUid)); 

vars.put(&quot;APICreationSuccessCount&quot;, &quot;0&quot;);
vars.put(&quot;APPCreationSuccessCount&quot;, &quot;0&quot;);
vars.put(&quot;APPKeyGenerationSuccessCount&quot;, &quot;0&quot;);
vars.put(&quot;subscriptionsCreationSuccessCount&quot;, &quot;0&quot;);
</stringProp>
